# Snakes Subsonic
# script for QuickBMS http://quickbms.aluigi.org

comtype gzip
findloc INFO_OFF binary "pkg\0\0\0\0\0"
goto INFO_OFF
idstring "pkg\0\0\0\0\0"
get FILES long
getdstring DUMMY 0x24
for i = 0 < FILES
    getdstring NAME 0x20
    get ID long
    get SIZE long
    get ZSIZE long
    get OFFSET long
    get ZERO long
    math OFFSET + INFO_OFF
    if SIZE == ZSIZE
        log NAME OFFSET SIZE
    else
        clog NAME OFFSET ZSIZE SIZE
    endif
next i
